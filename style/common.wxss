/**button*/

.btn-small, .btn-big {
  box-sizing: border-box;
  padding: 0 10rpx;
  font-size: 40rpx;
  color: #999;
  text-align: center;
  background-color: #fff;
  border: solid 1rpx #bfbfbf;
}

.btn-small {
  min-width: 128rpx;
  height: 50rpx;
  font-size:30rpx;
  line-height: 50rpx;
  border-radius: 25rpx;
}

.btn-big {
  min-width: 300rpx; 
  padding:25rpx;
  border-radius: 50rpx;
  width: 100%;
}

.btn-big image{
  width: 46rpx;
  height: 46rpx;
  vertical-align: middle;
  margin-right: 10rpx;
}

.btn-small.red, .btn-big.red,.btn-big.gray {
  color: #fff;
  border: solid 1rpx #ff5346;
  background-color: #ff5346;
}
.btn-big.gray{
   border: solid 1rpx #999;
   background-color: #999;
}
.btn-big.org {
  color: #81511c;
  font-size:38rpx;
  font-weight: 500;
  border: solid 1rpx #ffcd1e;
  background-color: #ffcd1e; 
  border-radius: 50px;
}
.btn-big.red {
  border-radius: 50px;
}
.btn-big.green {
  color: #fff;
  border: solid 1rpx #48ba82;
  background-color: #48ba82;
}

.btn-big.green.btn-border {
  color: #48ba82;
  background: #fff;
}
